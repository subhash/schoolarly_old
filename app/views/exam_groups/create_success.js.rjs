page << "closeDialogs();"
page << "jQuery('.tabs').tabs('select','#exams-tab')"
div_name='exams_of_' + @klass.id.to_s
if @klass.exam_groups.size ==1
  page.replace(div_name, :partial => 'exams/exams', :object => @exam_group.exams.group_by{|e| e.exam_group}, :locals => {:entity => @klass})
else
  page.insert_html(:top, div_name, :partial => 'exams/exams', :object => @exam_group.exams.group_by{|e| e.exam_group}, :locals => {:entity => @klass})
end