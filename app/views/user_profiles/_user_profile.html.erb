   <table class="ui-state-default center">
        <caption>
            User Details
        </caption>
        <% if !user_profile.nil? %>
        <tr>
            <td>
                <b>User Name:</b>
            </td>
            <td>
                <%=h user_profile.name.to_s %>
            </td>
        </tr>
        <tr>
            <td>
                <b>Address:</b>
            </td>
            <td>
                <%=h user_profile.address.address_line_1 %>
                <br/>
                <%=h user_profile.address.address_line_2 %>
                <br/>
                <%=h user_profile.address.city %>
                <br/>
                <%=h user_profile.address.state %>
                <br/>
                <%=h user_profile.address.country %>
            </td>
        </tr>
        <tr>
            <td>
                <b>Pincode:</b>
            </td>
            <td>
                <%=h user_profile.pincode %>
            </td>
        </tr>
        <tr>
            <td>
                <b>Phone Landline:</b>
            </td>
            <td>
                <%=h user_profile.phone_landline %>
            </td>
        </tr>
        <tr>
            <td>
                <b>Phone Mobile:</b>
            </td>
            <td>
                <%=h user_profile.phone_mobile %>
            </td>
        </tr>
        <% else %>
        <tr>
            <td colspan=2>
                <blockquote>
                    Profile Unavailable
                </blockquote>
            </td>
        </tr>
        <% end %>
    </table>