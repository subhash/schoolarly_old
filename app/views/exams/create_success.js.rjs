page << "closeDialogs();"
div_name='exam_group_' + @exam.exam_group.id.to_s
if @exam.exam_group.exams.size==1 
  page.replace_html div_name, :partial =>'exam_group', :locals => {:exam_group => @exam.exam_group, :exam_collection => @exam.exam_group.exams}
else
   new_div_name = "exam_" +  @exam.id.to_s
  page.insert_html :bottom, div_name, :text => '<tr id="' + new_div_name + '"><tr>'
  page.replace_html new_div_name, :partial =>'exam', :object => @exam
end