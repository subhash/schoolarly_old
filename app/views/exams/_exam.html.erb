	<tr id="exam_<%=exam.id%>">
	    <td class="ui-state-default">
	    	<%=exam.subject.name%>
	    </td>
	    <td class="ui-state-default">
	    	<%=exam.start_time%>
	    </td>
	    <td class="ui-state-default">
	    	<%=exam.end_time%>
	    </td>
		<td class="ui-state-default">
	    	<%=exam.venue%>
	    </td>
		<td class="ui-state-default">
	    	<%=exam.max_score%>
	    </td>
		<td class="ui-state-default">
	    	<%=exam.pass_score%>
	    </td>
		<td class="ui-state-default">
	        <%= link_to_remote 'Edit', :url => {:controller => 'exams', :action => 'delete', :id => exam}, :html => {:class => "ui-icon ui-icon-pencil", :title => 'Edit'}, :confirm => 'Are you sure?' %>
	    </td>
	    <td class="ui-state-default">
			<%= link_to_remote 'Delete', :url => {:controller => :exams, :action => 'delete',:id => exam}, :html => {:class => "ui-icon ui-icon-circle-close", :title => 'Delete'}, :confirm => 'Are you sure?' %>
	    </td>
		<td class="ui-state-default">
        	<%=link_to 'Enter Score', {:controller => 'scores', :action => 'new', :exam => exam}, {:class => "ui-icon ui-icon-circle-zoomin", :title => 'View/Edit Scores'}%>
	    </td>	
	</tr>
