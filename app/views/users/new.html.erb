<h1>Register</h1>
<% form_for @user, :url => account_path do |f| %>
<%= f.error_messages %>
<%= render :partial => "form", :object => f %>
<select name="user_type">
<%= options_for_select [['Teacher','Teacher'],['Student','Student'],['Parent','Parent'],['School','School']]%>
</select>
<%= f.submit "Register" %>
<% end %>