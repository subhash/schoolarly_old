<div id="school_tabs" class="tabs">
    <ul>
        <li>
            <a href="#classes-tab">Classes</a>
        </li>
        <li>
            <a href="#students-tab">Students</a>
        </li>
        <li>
            <a href="#teachers-tab">Teachers</a>
        </li>
        <li>
            <a href="#exams-tab">Exams</a>
        </li>
    </ul>
	<div id="main-dialog" title="Schoolarly" class="jquery-dialog" style ="display: none;"></div>
    <div id="classes-tab">
        <%= render :partial => 'klasses/klasses', :object => @klasses %>
    </div>
    <div id="students-tab">
        <%= render :partial => 'students/students', :object => @students %>
    </div>
    <div id="teachers-tab">
        <%=render :partial => 'teachers/teachers', :object => @teachers, :locals => {:subjects => @subjects} %>
    </div>
    <div id="exams-tab">
        <%=render :partial => 'exam_groups/exam_groups', :object => @exam_groups, :locals => {:school => @school} %>
    </div>
</div>
