<h3>Listing exams under <%=@exam_group.description %></h3>
    <table id="exam_list_table">
	<%if !@exams.nil? && !@exams.empty?%>
			<tr>
		    	<th>Subject</th><th>Start Time</th><th>End Time</th><th>Venue</th><th>Max Score</th><th>Pass Score</th>
			</tr>
		    <%=render :partial => "exam", :collection => @exams%>
			</table>
		<div class="button ui-state-default ui-corner-all float_left">
        <%= link_to_remote 'Edit', :url => {:controller => :schools, :action => 'exam_group_edit', :id=>@exam_group} %>
		</div>
	<%else%> 
	<tr><td>
		<blockquote>
	    	No exam added yet
		</blockquote>
		</td>
		</tr>
		</table>
	<%end%>
	
